{
	"name": "MA-Scraper",
	"description": "Tool for extracting band-related data from Encyclopaedia Metallum.",
	"version": "1.0.0",
	"private": true,
	"license": "UNLICENSED",
	"repository": "Alhadis/MA-Scraper",
	"directories": {"lib": "./src", "man": "./man"},
	"engines": { "node": ">=5.0.0" },
	"preferGlobal": true,
	"dependencies": {
		"jsdom": "*",
		"node-fetch": "*",
		"tough-cookie": "*",
		"get-options": "*",
		"mkdirp": "*"
	},
	"devDependencies": {
		"babel-core": ">=6.1.2",
		"babel-preset-es2015": ">=6.1.2",
		"babel-preset-stage-0": ">=6.1.2",
		"babel-preset-stage-1": ">=6.1.2"
	},
	
	"babel":{
		"plugins": [
			"transform-es2015-destructuring",
			"transform-es2015-modules-commonjs",
			"transform-es2015-parameters",
			"transform-es2015-sticky-regex",
			"transform-es2015-unicode-regex",
			"transform-function-bind",
			"transform-class-properties"
		]
	}
}
